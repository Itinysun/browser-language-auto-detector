{
  "name": "browser-language-auto-detector",
  "version": "2.0.5",
  "description": "🌍 Intelligent browser language detection library with 100+ languages support, RFC 5646 compliant, LRU cache optimized, TypeScript ready",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "clean": "rm -rf lib dist node_modules/.cache",
    "pre-release": "./scripts/pre-release-check.sh",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "release": "release-it",
    "release:dry": "release-it --dry-run",
    "verify-release": "./scripts/verify-release.sh",
    "prepublishOnly": "pnpm run clean && pnpm run lint && pnpm format && pnpm run test && pnpm run build"
  },
  "keywords": [
    "browser",
    "language",
    "detector",
    "i18n",
    "internationalization",
    "localization",
    "RTL",
    "bcp47",
    "rfc5646",
    "intl",
    "locale",
    "typescript",
    "performance",
    "cache"
  ],
  "authors": [
    "itinysun"
  ],
  "bugs": {
    "url": "https://github.com/Itinysun/browser-language-auto-detector/issues"
  },
  "homepage": "https://github.com/Itinysun/browser-language-auto-detector#readme",
  "license": "MIT",
  "files": [
    "lib",
    "README.md",
    "README-EN.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@jest/globals": "^29.7.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.0"
  },
  "packageManager": "pnpm@10.13.1"
}
